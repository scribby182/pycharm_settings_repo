    <keyboard-shortcut first-keystroke="alt q" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt right" />
  </action>
  <action id="GotoNextBookmark">
    <keyboard-shortcut first-keystroke="shift ctrl f3" />
  </action>
  <action id="HideSideWindows">
    <keyboard-shortcut first-keystroke="shift ctrl escape" />
  </action>
  <action id="MoveLineDown" />
  <action id="MoveLineUp" />

    <keyboard-shortcut first-keystroke="shift ctrl w" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="shift alt 8" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift alt 2" />
  </action>
  <action id="Table-selectPreviousRowExtendSelection" />
  <action id="Tree-selectPreviousExtendSelection" />
  <action id="UsageView.Remove">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="XDebugger.MoveWatchDown" />
  <action id="copilot.applyInlays">
    <keyboard-shortcut first-keystroke="ctrl alt enter" />
  </action>
</keymap>